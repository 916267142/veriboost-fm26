add-batfish-option haltonconverterror
add-batfish-option haltonparseerror
add-batfish-option loglevel fatal
add-batfish-option initinfo false

init-snapshot dataset/ospf/bics bics-ospf
get smt-generate-properties configPath="ospf/bics"

init-snapshot dataset/ospf/columbus columbus-ospf
get smt-generate-properties configPath="ospf/columbus"

